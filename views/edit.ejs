<%- include manageHeader %>
<!--<nav>
    tag 列表
</nav>-->
<form class="m-blog-form" id="js-editBlog">
    <div class="blog-title" >
        <input type="text" name="title" placeholder="标题 ^_^"/>
    </div>
    <!--<div class="m-tool-bar tool-bar">-->

    <!--</div>-->
    <div class="m-tag tag">
        <div class="tokens">
            <span class="token">tag</span>
            <input maxlength="20" class="taginput" style="width: 60px;">
        </div>
    </div>

    <div class="blog-content" >
        <textarea type="text" name="content" placeholder="markdown 编辑内容">  </textarea>
    </div>
    <div class="u-btn-wrap action-bar">
        <button class="u-btn" type="" id="js-scan">预&nbsp;览</button>
        <!--<button class="u-btn" type="" id="js-scan">👀</button>-->
        <button class="u-btn u-light-btn" type="submit" id="js-saveBlog">发&nbsp;布</button>
    </div>
</form>

<%- include manageFooter %>